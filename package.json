{
  "name": "redux-auth",
  "version": "5.4.4",
  "description": "Set of authentication components and containers such as: Sign In, Sign Up, Sign Out, Reset Password, authorization wrappers",
  "repository": "github:HealthByRo/redux-auth",
  "author": "Roman Health",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run lint & npm run test",
    "build": "rm -rf lib && npm run quick-build",
    "quick-build": "babel -d lib/ src/ --ignore \"**/*.test.js\",\"**/*.stories.js\"",
    "test": "jest --config=.jest.json --coverage",
    "test:watch": "jest --config=.jest.json --no-cache --watchAll",
    "test:badge": "npm run test && node ./scripts/coverage.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern 'flow-typed/' --ignore-pattern 'lib/' --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prerelease": "git checkout master && git pull && git checkout develop && git pull",
    "release": "npm version",
    "postrelease": "git push && git push --tags && git checkout master && git merge develop && git push && git checkout develop"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "decko": "^1.2.0",
    "humps": "^2.0.1",
    "moment": "^2.24.0",
    "query-string": "^6.8.3",
    "react-form-fields": "https://github.com/HealthByRo/react-form-fields#^1.2.0",
    "react-helmet": "^5.2.1",
    "react-recaptcha": "^2.3.10",
    "redux-auth-wrapper": "^2.1.0",
    "redux-form": "^7.2.3",
    "reselect": "^4.0.0",
    "validators": "https://github.com/HealthByRo/validators#^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "7.0.0",
    "@kadira/storybook": "^2.35.3",
    "@redux-saga/testing-utils": "^1.0.5",
    "api-client": "https://github.com/HealthByRo/api-client#^1.3.6",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "lcov2badge": "^0.1.0",
    "lint-staged": "^9.2.3",
    "mockdate": "^2.0.5",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "5.0.0-alpha.8",
    "react-unit-testing-utils": "https://github.com/HealthByRo/react-unit-testing-utils#^3.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-saga-test-plan": "^3.7.0"
  },
  "homepage": "https://github.com/HealthByRo/redux-auth/"
}
