{
  "name": "redux-auth",
  "version": "2.5.1",
  "description": "Set of authentication components and containers such as: Sign In, Sign Up, Sign Out, Reset Password, authorization wrappers",
  "repository": "git@bitbucket.org:arabellatech/redux-auth.git",
  "author": "Arabella Front-End Team <team@arabel.la>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run lint & npm run test:badge",
    "build": "rm -rf lib && babel --presets es2015,stage-0 --plugins transform-decorators-legacy -d lib/ src/ --ignore \"**/*.test.js\",\"**/*.stories.js\"",
    "test": "jest --config=.jest.json --coverage ",
    "test:watch": "jest  --config=.jest.json --watchAll",
    "test:badge": "yarn run test && node ./scripts/coverage.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern 'flow-typed/' --ignore-pattern 'lib/'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "immutable": "^3.8.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.18.1",
    "query-string": "^5.0.0",
    "react-form-fields": "https://bitbucket.org/arabellatech/react-form-fields#^1.2.0",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.4",
    "react-router": "3.0.0",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.1",
    "redux-auth-wrapper": "^1.1.0",
    "redux-form": "^6.6.3",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.0",
    "validators": "https://bitbucket.org/arabellatech/validators#^2.0.0"
  },
  "peerDependencies": {
    "api-client": "https://bitbucket.org/arabellatech/api-client#^1.3.0",
    "react-intl": "^2.2.3",
    "redux-saga": "^0.14.8",
    "reselect": "^3.0.0",
    "validators": "https://bitbucket.org/arabellatech/validators#^2.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "api-client": "https://bitbucket.org/arabellatech/api-client#^1.3.0",
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "lcov2badge": "^0.1.0",
    "lint-staged": "^3.4.0",
    "mockdate": "^2.0.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "^2.2.3",
    "react-test-renderer": "^15.5.4",
    "react-unit-testing-utils": "https://bitbucket.org/arabellatech/react-unit-testing-utils#^1.2.0",
