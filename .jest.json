{
  "testURL": "https://test.com/",
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/**/*.test{s}.{js,jsx}"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "testRegex": "src/.*\\.test[s]?\\.js$",
  "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js"
}